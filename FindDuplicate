tortoise = array[0]

while True:
    tortoise = nums[tortoise]
    haare = nums[nums[haare]]
    print(f"{haare} - {tortoise}")
    time.sleep(0.2)
    if haare == tortoise:
        ShowToast("Match Found!", str(haare), 10)
        break
